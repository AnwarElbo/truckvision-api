{
  "name": "xolvio/truckvision-api",
  "description": "Connection with the Truckvision webservice",
  "keywords": ["truckvision", "webservice"],
  "license" : "MIT",
  "authors": [
    {
      "name": "Anwar el Bouhdifi",
      "email": "anwar.elbouhdifi@gmail.com"
    }
  ],
  "require": {
    "php": ">=7.1",
    "illuminate/container": "5.7.x|5.8.x",
    "illuminate/database": "5.7.x|5.8.x",
    "illuminate/support": "5.7.x|5.8.x",
    "illuminate/http": "5.7.x|5.8.x",
    "guzzlehttp/guzzle": "^6.3.3",
    "symfony/var-dumper": "4.x",
    "spatie/array-to-xml": "^2.8",
    "ext-soap": "*",
    "ext-curl": "*",
    "ext-simplexml": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "^7",
    "imsop/simplexml_debug": "v1.0.0",
    "xolvio/codestyle": "^1.5"
